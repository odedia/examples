{
  "connector.class": "SnowflakeSink",
  "name": "${ETL_CONFLUENT_CONNECTOR_NAME}",
  "kafka.auth.mode": "KAFKA_API_KEY",
  "kafka.api.key": "${ETL_CONFLUENT_API_KEY}",
  "kafka.api.secret": "${ETL_CONFLUENT_API_SECRET}",
  "topics": "${ETL_CONFLUENT_KAFKA_TOPIC_NAME}",
  "input.data.format": "JSON",
  "buffer.flush.time": "10",
  "snowflake.url.name": "${ETL_SNOWFLAKE_URL}",
  "snowflake.user.name": "confluent",
  "snowflake.private.key": "${ETL_SNOWFLAKE_KEY}",
  "snowflake.database.name": "MYDB3",
  "snowflake.schema.name": "PUBLIC",
  "tasks.max": "1"
}
