{
  "name": "ETL",
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "src_type": {
        "type": "string",
        "enum": ["confluentcloud"]
      },
      "src_bootstrapServers": {
        "type": "string"
      },
      "src_topic": {
        "type": "string"
      },
      "src_dataformat": {
        "type": "string",
        "enum": ["json", "string"]
      },
      "dst_type": {
        "type": "string",
        "enum": ["snowflake"]
      },
      "dst_user": {
        "type": "string"
      },
      "dst_db": {
        "type": "string"
      },
      "dst_schema": {
        "type": "string",
        "enum": ["PUBLIC"]
      },
      "secrets_url": {
        "type": "string",
        "enum": ["keyring"]
      },
      "job": {
        "type": "string"
      },
      "status": {
        "type": "string",
        "enum": ["Submitted", "Up", "Deleted", "PAUSED", "RUNNING", "UNASSIGNED", "FAILED", "PROVISIONING"]
      }
    },
    "required": [
      "name"
    ]
  }
}
